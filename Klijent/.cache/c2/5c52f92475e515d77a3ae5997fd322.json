ault();\r\n\r\n        fetch(\"http://localhost:5012/api/login\", {\r\n            method: \"POST\",\r\n            body: JSON.stringify({\r\n                email: email,\r\n                lozinka: lozinka\r\n            }),\r\n            headers: { \"Content-type\": \"application/json;charset=UTF-8\" }\r\n        })\r\n            .then((resp) => resp.json())\r\n            .then((data) => {\r\n                if (data.accessToken) {\r\n                    localStorage.setItem(\"token\", data.accessToken);\r\n                    localStorage.setItem(\"uloga\", data.uloga);\r\n                    localStorage.setItem(\"email\", data.korisnik);\r\n                    navigate('/');\r\n                } else {\r\n                    console.log(\"Authentication error\");\r\n                }\r\n            })\r\n            .catch((err) => console.log(err));\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <form onSubmit={(e) => { handleLogin(e); }}>\r\n                <label htmlFor=\"email\">Email</label>\r\n                <input\r\n                    type=\"text\"\r\n                    value={email}\r\n                    onChange={zaEmail}\r\n                    onBlur={zaEmail}\r\n                ></input>\r\n\r\n                <label htmlFor=\"lozinka\">Lozinka</label>\r\n                <input\r\n                    type=\"password\"\r\n                    value={lozinka}\r\n                    onChange={zaLozinku}\r\n                    onBlur={zaLozinku}\r\n                ></input>\r\n\r\n                <button type=\"submit\">Login</button>\r\n            </form>\r\n        </div>\r\n    )\r\n};\r\n\r\nexport default Login;"},"lineCount":null}},"error":null,"hash":"b7c337db14030f8f565c191039331e92","cacheData":{"env":{}}}